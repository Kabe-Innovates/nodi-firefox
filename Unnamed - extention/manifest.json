{
  "manifest_version": 3,
  "name": "Unnamed - extention (as of now)",
  "version": "0.1.0",
  "description": "Blocks distracting websites when the user is inside a Productive Zone.",
  "permissions": [
    "geolocation",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "focus-shield@example.com"
    }
  },
  "action": {
    "default_title": "Focus Shield",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
